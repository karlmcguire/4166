<% include header %>

    <div class="container">
      <nav aria-label="breadcrumb" class="mt-4">
        <ol class="breadcrumb">
          <li><span>Connections</span></li>
        </ol>
      </nav>

      <div class="pt-4 pb-2">
        <h1 class="text-secondary pb-4">Connections</h1> 
        <ul>
          <% for(var i = 0; i < data.length; i++) { %>
                <li>
                    <a href="/connection/<%= data[i].id %>">
                        <h4><%= data[i].title %></h4>
                    </a>
                </li>
          <% } %>
        </ul>
      </div>

<% include footer %>
