<% include header %>

      <div class="pt-4 pb-2">
        <h1 class="text-secondary pb-4">Saved Connections</h1> 
        <table class="table">
  <thead>
    <tr>
      <th scope="col">Connection</th>
      <th scope="col">Category</th>
      <th scope="col">Going?</th>
      <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
  <% for(var i = 0; i < data.length; i++) { %>
    <tr>
        <td><%= data[i].connection.title %></td>
        <td><%= data[i].connection.name %></td>
        <td><%= data[i].connection.rsvp %></td>
        <td>
            <div class="btn-group" role="group">
                <a href="/connection/<%= data[i].connection.id %>" class="btn btn-primary">Update</a>
                <a href="/deleteConnection/<%= data[i].connection.id %>" class="btn btn-danger">Delete</a>
            </div>
        </td>
    </tr>
  <% } %>
  </tbody>
</table>
      </div>

<% include footer %>
